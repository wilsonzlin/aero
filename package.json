{
  "name": "aero",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve:coi": "vite build --emptyOutDir && vite preview --host 127.0.0.1 --port 4173 --strictPort",
    "gen:scancodes": "node tools/gen_scancodes/gen_scancodes.mjs",
    "typecheck": "tsc -p tsconfig.json",
    "test": "npm run test:unit",
    "demo": "node web/demo/server.js",
    "bench:gpu": "node --experimental-strip-types bench/gpu_bench.ts",
    "compare:gpu": "node --experimental-strip-types scripts/compare_gpu_benchmarks.ts",
    "test:protocol": "node --test --experimental-strip-types emulator/protocol/tests/*.test.ts",
    "test:unit": "npm run typecheck && node --test --experimental-strip-types --loader ./scripts/ts-strip-loader.mjs backend/aero-gateway/test/originGuard.test.ts backend/aero-gateway/test/tcp*.test.ts backend/aero-gateway/src/**/*.test.ts tests/*.test.js bench/history.test.js packages/aero-stats/test/*.test.js web/test/*.test.ts emulator/protocol/tests/*.test.ts",
    "test:e2e": "playwright test",
    "test:coi": "playwright test tests/e2e/coi.spec.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@types/node": "^20.10.6",
    "@webgpu/types": "^0.1.68",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "ws": "^8.18.0"
  }
}
