{
  "name": "aero",
  "private": true,
  "type": "module",
  "workspaces": [
    "backend/aero-gateway",
    "bench",
    "net-proxy",
    "packages/*",
    "proxy/webrtc-udp-relay/e2e",
    "server",
    "services/image-gateway",
    "tools/disk-streaming-browser-e2e",
    "tools/net-proxy-server",
    "tools/perf",
    "tools/range-harness",
    "web"
  ],
  "engines": {
    "node": ">=22.11.0 <23"
  },
  "scripts": {
    "check:node": "node scripts/check-node-version.mjs",
    "predev": "npm run check:node",
    "prebuild": "npm run check:node",
    "dev": "vite --config vite.harness.config.ts",
    "predev:harness": "npm run check:node",
    "predev:web": "npm run check:node",
    "build": "vite build --config vite.harness.config.ts",
    "build:prod": "npm -w web run build:wasm && npm run build",
    "prebuild:harness": "npm run check:node",
    "prebuild:web": "npm run check:node",
    "preview": "vite preview --config vite.harness.config.ts",
    "serve:coi": "vite build --config vite.harness.config.ts --emptyOutDir && vite preview --config vite.harness.config.ts --host 127.0.0.1 --port 4173 --strictPort",
    "dev:web": "vite web --config web/vite.config.ts",
    "build:web": "vite build web --config web/vite.config.ts",
    "preview:web": "vite preview web --config web/vite.config.ts",
    "serve:coi:web": "vite build web --config web/vite.config.ts --emptyOutDir && vite preview web --config web/vite.config.ts --host 127.0.0.1 --port 4173 --strictPort",
    "dev:harness": "vite --config vite.harness.config.ts",
    "build:harness": "vite build --config vite.harness.config.ts",
    "preview:harness": "vite preview --config vite.harness.config.ts",
    "serve:coi:harness": "vite build --config vite.harness.config.ts --emptyOutDir && vite preview --config vite.harness.config.ts --host 127.0.0.1 --port 4173 --strictPort",
    "gen:scancodes": "node tools/gen_scancodes/gen_scancodes.mjs",
    "check:scancodes": "node tools/gen_scancodes/check_generated.mjs",
    "pretypecheck": "npm run check:node",
    "typecheck": "tsc -p tsconfig.json && tsc -p web/tsconfig.json",
    "generate:mega-module": "node scripts/generate-mega-module.mjs",
    "bench:run": "node bench/run",
    "bench:browser": "node bench/run --scenario all --iterations 7",
    "bench:node": "node bench/run.js --out bench/results.json",
    "bench:update-baseline": "node bench/update-baseline",
    "demo": "node web/demo/server.js",
    "test": "npm run test:unit",
    "pretest:unit": "npm run check:node",
    "bench:compare": "node bench/compare --json",
    "bench:gpu": "node --experimental-strip-types --import ./scripts/register-ts-strip-loader.mjs bench/gpu_bench.ts",
    "bench:storage": "node --experimental-strip-types --import ./scripts/register-ts-strip-loader.mjs bench/runner.ts storage_io",
    "bench:gateway": "npm -w backend/aero-gateway run bench",
    "bench:gateway:smoke": "npm -w backend/aero-gateway run bench:smoke",
    "bench:gateway:nightly": "npm -w backend/aero-gateway run bench:nightly",
    "compare:gpu": "node --experimental-strip-types --import ./scripts/register-ts-strip-loader.mjs scripts/compare_gpu_benchmarks.ts",
    "compare:gateway": "node --experimental-strip-types --import ./scripts/register-ts-strip-loader.mjs scripts/compare_gateway_benchmarks.ts",
    "compare:storage": "node --experimental-strip-types --import ./scripts/register-ts-strip-loader.mjs bench/compare.ts",
    "compare:storage:legacy": "node --experimental-strip-types --import ./scripts/register-ts-strip-loader.mjs scripts/compare_storage_benchmarks.ts",
    "test:protocol": "node --test --experimental-strip-types --import ./scripts/register-ts-strip-loader.mjs emulator/protocol/tests/*.test.ts",
    "test:unit": "npm run typecheck && node -e \"require('node:fs').mkdirSync('test-results', { recursive: true })\" && node --test --test-reporter=spec --test-reporter-destination=stdout --test-reporter=junit --test-reporter-destination=test-results/node-junit.xml --experimental-strip-types --import ./scripts/register-ts-strip-loader.mjs backend/aero-gateway/test/originGuard.test.ts backend/aero-gateway/test/cookies.test.ts backend/aero-gateway/test/sessionToken.vectors.test.ts backend/aero-gateway/test/udpRelayJwt.vectors.test.ts backend/aero-gateway/test/tcp*.test.ts tests/*.test.js tests/fixtures/servers.limits.test.ts tests/playwright_static_server.limits.test.ts tests/web_storage_http_headers.test.ts tests/web_remote_disk_probe_headers.test.ts bench/history.test.js bench/tests/*.test.ts tools/perf/tests/*.test.js tools/range-harness/test/core.test.js packages/aero-stats/test/*.test.js web/test/*.test.ts emulator/protocol/tests/*.test.ts && npm -w web run test:unit -- --reporter=default --reporter=junit --outputFile=../test-results/vitest-junit.xml",
    "test:unit:coverage": "vitest run --coverage --config vitest.config.ts",
    "pretest:unit:coverage": "npm run check:node",
    "pretest:e2e": "npm run check:node && npm -w web run wasm:build",
    "test:e2e": "playwright test",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:e2e:report": "playwright show-report",
    "test:coi": "playwright test tests/e2e/coi.spec.ts",
    "test:security-headers": "playwright test tests/e2e/security-headers.spec.ts",
    "check:security-headers": "node scripts/ci/check-security-headers.mjs",
    "test:webgpu": "playwright test --project=chromium-webgpu",
    "generate:goldens": "node tests/golden/generate_goldens.cjs",
    "check:goldens": "node scripts/ci/check-goldens.mjs",
    "test:gpu": "playwright test -c playwright.gpu.config.ts tests/e2e/playwright/gpu_golden.spec.ts",
    "wasm:ensure": "npm -w web run wasm:ensure",
    "wasm:build": "npm -w web run wasm:build",
    "wasm:build:single": "npm -w web run wasm:build:single",
    "wasm:build:threaded": "npm -w web run wasm:build:threaded",
    "wasm:build:dev": "npm -w web run wasm:build:dev",
    "wasm:build:release": "npm -w web run wasm:build:release",
    "wasm:size": "npm -w web run wasm:size"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@types/w3c-web-hid": "^1.0.6",
    "@types/node": "^20.11.30",
    "@webgpu/types": "^0.1.68",
    "@types/pixelmatch": "^5.2.6",
    "@types/pngjs": "^6.0.5",
    "@types/w3c-web-usb": "^1.0.13",
    "@vitest/coverage-v8": "^1.6.0",
    "esbuild": "^0.21.3",
    "fake-indexeddb": "^6.2.5",
    "jsdom": "^24.0.0",
    "license-checker-rseidelsohn": "4.3.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.10",
    "vitest": "^1.6.0",
    "wasm-pack": "^0.13.1",
    "wabt": "^1.0.37",
    "ws": "^8.18.0"
  }
}
