    driver.c \
    pci_interface.c \
    topology_miniport.cpp \
    virtio_pci_modern_wdm.c \
    virtio_snd_proto.c \
    virtiosnd_control.c \
    virtiosnd_queue_split.c \
    virtiosnd_tx.c \
    virtiosnd_hw.c \
    virtiosnd_dma.c \
    virtiosnd_intx.c \
    virtiosnd_sg.c \
    virtiosnd_sg_core.c \
    virtiosnd_sg_tx.c \
    ..\..\..\windows\virtio\common\virtqueue_split.c \
    ..\..\..\win7\virtio\virtio-core\portable\virtio_pci_cap_parser.c \
    ..\..\virtio\common\src\virtio_pci_contract.c \
    ..\..\..\win7\virtio\virtio-core\portable\virtio_pci_identity.c
