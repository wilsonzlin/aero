  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@vitest/coverage-v8": "^1.6.0",
    "@webgpu/types": "^0.1.68",
    "typescript": "^5.5.4",
    "vite": "^5.4.10",
    "vitest": "^1.6.0"
  }
}
