{
  "name": "aero-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.11.0 <23"
  },
  "scripts": {
    "build:wasm:microbench": "wasm-pack build --target web --release --out-dir src/wasm/aero_microbench ../crates/aero-microbench --locked",
    "wasm:build:threaded": "node ./scripts/build_wasm.mjs threaded",
    "wasm:build:single": "node ./scripts/build_wasm.mjs single",
    "wasm:ensure": "node ./scripts/ensure_wasm.mjs",
    "wasm:build": "npm run wasm:build:single && npm run wasm:build:threaded",
    "build:wasm": "npm run build:wasm:microbench && npm run wasm:ensure",
    "predev": "npm run build:wasm",
    "dev": "vite",
    "prebuild": "npm run build:wasm",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "pretypecheck": "npm run build:wasm",
    "typecheck": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "wasm:build:dev": "node ./scripts/build_wasm.mjs single dev",
    "wasm:build:release": "npm run wasm:build",
    "wasm:size": "node ./scripts/wasm-size.mjs"
  }
}
