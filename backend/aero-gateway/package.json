{
  "name": "aero-gateway",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.11.0 <23"
  },
  "scripts": {
    "check:node": "node ../../scripts/check-node-version.mjs",
    "predev": "npm run check:node",
    "dev": "tsx watch src/index.ts",
    "prelint": "npm run check:node",
    "lint": "tsc -p tsconfig.lint.json",
    "pretest": "npm run check:node",
    "test": "node ./scripts/run-tests.js",
    "pretest:e2e": "npm run check:node",
    "test:e2e": "playwright test",
    "test:property": "node ./scripts/run-tests.js test/property",
    "prebuild": "npm run check:node",
    "build": "tsc -p tsconfig.json",
    "prestart": "npm run check:node",
    "start": "node dist/index.js",
    "bench": "npm run build && node bench/run.mjs --mode local --json bench/results.json",
    "bench:nightly": "npm run build && node bench/run.mjs --mode nightly --json bench/results.json",
    "bench:smoke": "npm run build && node bench/run.mjs --mode smoke --json bench/results.json --assert"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "fastify": "^4.27.0",
    "prom-client": "^15.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "autocannon": "^7.15.0",
    "dns-packet": "^5.6.1",
    "fast-check": "^3.16.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4"
  }
}
