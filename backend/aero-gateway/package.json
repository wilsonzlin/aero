{
  "name": "aero-gateway",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.11.1 <21"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "lint": "tsc -p tsconfig.lint.json",
    "test": "node ./scripts/run-tests.js",
    "test:e2e": "playwright test",
    "test:property": "node ./scripts/run-tests.js test/property",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "bench": "npm run build && node bench/run.mjs --mode local --json bench/results.json",
    "bench:smoke": "npm run build && node bench/run.mjs --mode smoke --json bench/results.json --assert"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "fastify": "^4.27.0",
    "prom-client": "^15.1.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "autocannon": "^7.15.0",
    "dns-packet": "^5.6.1",
    "fast-check": "^3.16.0"
  }
}
